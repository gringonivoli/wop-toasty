{
  "name": "wop-toasty",
  "version": "0.1.6",
  "description": "Web Component built with Stencil to Show Dan Forden's Toasty from Mortal Kombat.",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/core": "^0.9.11"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gringonivoli/wop-toasty.git"
  },
  "author": "Maxi Nivoli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gringonivoli/wop-toasty"
  },
  "homepage": "https://github.com/gringonivoli/wop-toasty",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
